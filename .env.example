# Environment Variables

# SwipeOne API Configuration
SWIPEONE_API_KEY=your_api_key_here
SWIPEONE_API_BASE_URL=https://api.swipeone.com/api
API_TIMEOUT=30000

# Environment
NODE_ENV=development

# Optional: Default Workspace ID for testing
# In production with OAuth, this will be dynamically determined from the authenticated user
DEFAULT_WORKSPACE_ID=your_workspace_id_here

# Security Configuration
# Enable/disable rate limiting (true/false)
ENABLE_RATE_LIMITING=true

# Logging level (debug, info, warn, error)
# Use 'debug' in development, 'info' or 'warn' in production
LOG_LEVEL=info
